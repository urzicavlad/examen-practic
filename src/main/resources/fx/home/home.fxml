<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ro.ubbcluj.rentauto.controller.home.HomeController">

    <HBox>
        <Button fx:id="btnCars"
                prefHeight="100"
                prefWidth="100"
                onAction="#goToCarsOnClick"
                text="CARS"/>

        <Button fx:id="btnRents"
                prefHeight="100"
                prefWidth="100"
                onAction="#goToRentsOnClick"
                text="RENTS"/>
    </HBox>
</AnchorPane>
